{
  "version": "2.0.0",
  "command": "npm",
  "type": "shell",
  "presentation": {
      "echo": true,
      "reveal": "silent",
      "focus": false,
      "panel": "shared",
      "showReuseMessage": true,
      "clear": false
  },
  "args": ["run", "compile", "--loglevel", "silent"],
  "isBackground": true,
  "problemMatcher": "$tsc-watch"
}
